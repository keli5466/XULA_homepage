
<section class="slider">
	<div class="slide-viewer">
		<div class="slide-group">

			    {{#each Slide}}
					<div class="slide ">
						<h1 class="georgia heading-4">
							{{Quote}}
</h1>
						<img class="icon" src="{{ Image.src }}" alt"{{ Image.alt }}"/>
					</div>
			    {{/each}}
		</div>
	</div>
	<div class="slide-buttons"></div>
</section>



