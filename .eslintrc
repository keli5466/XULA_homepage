{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jquery": true,
  },
  "globals": {
    "window": true,
  },
  "rules": {
    "prefer-arrow-callback": 0,
    "no-var": 0,
    "one-var": 0,
    "no-use-before-define": 0,
    "one-var-declaration-per-line": 0,
    "indent": ["error", "tab"],
    "no-tabs": 0,
    "max-len": 0,
    "prefer-template": 0,
    "no-plusplus": 0,
    "class-methods-use-this": [0],
    "import/no-extraneous-dependencies": [2, { "devDependencies": true }],
  }
}
